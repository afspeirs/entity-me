{
  "name": "entity-me",
  "version": "1.10.0-develop.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "prebuild": "npm run semantic-release:dry",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "type": "npm run check",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "lint": "eslint .",
    "pwa": "npm run build && npm run preview",
    "semantic-release:dry": "semantic-release --dry-run --no-ci",
    "outdated": "npx npm-check-updates",
    "outdated:minor": "npx npm-check-updates -t minor",
    "update": "npm run outdated -- -u",
    "update:minor": "npm run outdated:minor -- -u"
  },
  "devDependencies": {
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.86.2",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.12.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.2",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.10.5",
    "@vite-pwa/sveltekit": "^0.6.7",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.18.0",
    "eslint-plugin-svelte": "^2.46.1",
    "globals": "^15.14.0",
    "lucide-svelte": "^0.471.0",
    "postcss": "^8.4.49",
    "semantic-release": "^24.2.1",
    "svelte": "^5.17.3",
    "svelte-check": "^4.1.4",
    "tailwindcss": "^3.4.17",
    "tailwindcss-safe-area": "^0.6.0",
    "tippy.js": "^6.3.7",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.19.1",
    "vite": "^6.0.7",
    "vitest": "^2.1.8"
  }
}
