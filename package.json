{
  "name": "entity-me",
  "version": "1.13.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "prebuild": "npm run semantic-release:dry",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "type": "npm run check",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "lint": "eslint .",
    "pwa": "npm run build && npm run preview",
    "semantic-release:dry": "semantic-release --dry-run --no-ci",
    "outdated": "npx npm-check-updates",
    "outdated:minor": "npx npm-check-updates -t minor",
    "update": "npm run outdated -- -u",
    "update:minor": "npm run outdated:minor -- -u"
  },
  "devDependencies": {
    "@eslint/compat": "^1.4.0",
    "@semantic-release/exec": "^7.1.0",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^5.4.0",
    "@sveltejs/adapter-static": "^3.0.10",
    "@sveltejs/kit": "^2.43.8",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.14",
    "@types/eslint": "^9.6.1",
    "@types/node": "^24.6.2",
    "@vite-pwa/sveltekit": "^1.0.0",
    "eslint": "^9.37.0",
    "eslint-plugin-svelte": "^3.12.4",
    "globals": "^16.4.0",
    "lucide-svelte": "^0.544.0",
    "melt": "^0.41.0",
    "postcss": "^8.5.6",
    "semantic-release": "^24.2.9",
    "svelte": "^5.39.8",
    "svelte-check": "^4.3.2",
    "tailwindcss": "^4.1.14",
    "tailwindcss-safe-area": "^1.1.0",
    "tippy.js": "^6.3.7",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^7.1.9"
  }
}
