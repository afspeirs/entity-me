{
  "name": "entity-me",
  "version": "1.10.0-develop.1",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "start": "vite dev",
    "prebuild": "npm run semantic-release:dry",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "test:unit": "vitest",
    "test": "npm run test:unit -- --run",
    "lint": "eslint .",
    "pwa": "npm run build && npm run preview",
    "semantic-release:dry": "semantic-release --dry-run --no-ci",
    "outdated": "npx npm-check-updates",
    "outdated:minor": "npx npm-check-updates -t minor",
    "update": "npm run outdated -- -u",
    "update:minor": "npm run outdated:minor -- -u"
  },
  "devDependencies": {
    "@iconify/svelte": "^4.0.2",
    "@melt-ui/pp": "^0.3.2",
    "@melt-ui/svelte": "^0.85.0",
    "@semantic-release/exec": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@stylistic/eslint-plugin": "^2.10.1",
    "@sveltejs/adapter-static": "^3.0.6",
    "@sveltejs/kit": "^2.7.4",
    "@sveltejs/svelte-virtual-list": "^3.0.1",
    "@sveltejs/vite-plugin-svelte": "^4.0.0",
    "@tailwindcss/typography": "^0.5.15",
    "@types/eslint": "^9.6.1",
    "@types/node": "^22.8.6",
    "@vite-pwa/sveltekit": "^0.6.6",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.14.0",
    "eslint-plugin-svelte": "^2.46.0",
    "globals": "^15.13.0",
    "postcss": "^8.4.47",
    "semantic-release": "^24.2.0",
    "svelte": "^5.1.9",
    "svelte-check": "^4.0.5",
    "tailwindcss": "^3.4.14",
    "tailwindcss-safe-area": "^0.6.0",
    "tippy.js": "^6.3.7",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.12.2",
    "vite": "^5.4.10",
    "vitest": "^2.1.4"
  }
}
